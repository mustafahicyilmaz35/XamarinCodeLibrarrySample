        <StackLayout>
            <Label Text="App Theme"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" x:Name="LabelAppTheme" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>